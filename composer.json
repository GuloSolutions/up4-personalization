{
    "name": "gulo-solutions/up4-personalization",
    "description": "up4 Facebook login, Survey wizard and Personalization",
    "keywords": ["API", "up4-personalization"],
    "version": "1.0.1",
    "license": "MIT",
    "type": "wordpress-plugin",
    "require": {
        "illuminate/database": "5.4",
        "facebook/graph-sdk": "^5.6",
        "robmorgan/phinx": "^0.9.2",
        "guzzlehttp/guzzle": "^6.3",
        "composer/installers": "~1.0",
        "tedivm/stash": "0.14.*",
        "nesbot/carbon": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Models\\": "app/Models",
            "Controllers\\": "app/Controllers",
            "Migrations\\":"app/Migration"
        },
        "extra": {
            "installer-paths": {
                "./wp-content/plugins/{$name}/": [
                    "type:wordpress-plugin"
                    ]
                }
        }
    }
}
